[program:<%= node['deploy'][@webapp_name][:environment][:dns_name] %>]
command=/usr/bin/nodejs server.js
process_name=<%= node['deploy'][@webapp_name][:environment][:dns_name] %>%(process_num)s
numprocs=1
directory=/srv/www/<%= node['deploy'][@webapp_name] %>/current/
umask=022
autostart=true
autorestart=true
startsecs=2
startretries=3